		<![CDATA[select 	pr.codigo,
	pr.nombre_producto,
	pr.precio_compra,
	pr.precio_venta,
	pr.stuckTienda,
	pr.stuck_minimo_tienda,
	pf.nombre_familia,
	f.nombre_fabricante
from 
	tbl_producto pr
inner join 
	tbl_producto_familia pf on pf.idproducto_familia = pr.idfamilia
inner join 
	tbl_fabricante f on f.idfabricante = pr.idfabricante
